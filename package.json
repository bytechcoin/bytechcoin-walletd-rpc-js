{
  "name": "bytechcoin-walletd-rpc-js",
  "version": "0.7.2",
  "description": "Wrapper for the BytechCoin walletd daemon JSON-RPC interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/bytechcoin/bytechcoin-walletd-rpc-js.git"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run init",
    "build": "babel ./src -d ./dist --ignore spec.js",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-core/register ./src/**/*.spec.js",
    "test:single": "mocha --compilers js:babel-core/register"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "author": "bebop",
  "license": "Beerware",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.0"
  },
  "dependencies": {
    "popsicle": "^9.2.0"
  }
}
